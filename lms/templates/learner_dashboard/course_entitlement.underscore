<div class="message is-shown">
    <h5> You must select a course run for uuid <%-entitlementUUID %>.</h5>
    <select>
        <option selected="selected">Select session later</option>
        <% for (var i=0; i< availableSessions.length; i++) { %>
            <option><%-availableSessions[i] %></option>
        <% } %>
    </select>
    <button class="btn btn-primary"> Enroll In Session </button>
</div>